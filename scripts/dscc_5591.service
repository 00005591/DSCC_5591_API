[Unit]
Description=Example .NET Web API App running on Linux
[Service]
WorkingDirectory=/var/www/dscc_5591
ExecStart=/usr/bin/dotnet /var/www/dscc_5591/bin/Debug/netcoreapp3.0/DSCC_CW1_5591.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=dotnet
User=apache
[Install]
WantedBy=multi-user.target

